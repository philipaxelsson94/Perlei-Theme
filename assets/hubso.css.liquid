  .bg-quaternary-background :focus {
    outline-width: var(--text-width);
    outline-style: solid;
    outline-color: var(--color-quaternary-accent);
    outline-offset: 0.25rem;
  }
  .bg-quaternary-background .increase-target:focus::before {
    outline: var(--text-width) solid -webkit-focus-ring-color;
    outline: var(--text-width) solid var(--color-quaternary-accent);
    outline-offset: calc(-1 * var(--text-width));
  }
  .bg-quaternary-background .custom-input:focus + label {
    outline: var(--text-width) solid -webkit-focus-ring-color;
    outline: var(--text-width) solid var(--color-quaternary-accent);
  }
  .bg-quaternary-background .product-media-object:focus {
    outline: 0;
  }
  .bg-quaternary-background .product-media-object:focus:after {
    width: 100%;
    height: 100%;
    content: '';
    outline: var(--text-width) solid -webkit-focus-ring-color;
    outline: var(--text-width) solid var(--color-quaternary-accent);
    outline-offset: calc(-1 * var(--text-width));
    position: absolute;
    top: 0;
    left: 0;
  }
