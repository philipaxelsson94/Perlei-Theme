/* --- Weglot Switchers --- */

.weglot-switcher {
  display: flex;
  align-items: center; 
}
.weglot-switcher .country-selector .wg-li {
  font-size: var(--base-font-size);
}
.weglot-switcher .wg-drop.country-selector {
  background-color: transparent;
}
.weglot-switcher .wg-drop.country-selector ul {
  top: 57px;
}
.weglot-switcher .wg-drop.country-selector .wgcurrent {
  border: none;
}
.weglot-switcher .wg-drop.country-selector .wgcurrent a {
  padding-right: 30px;
}
.weglot-switcher .wg-drop.country-selector .wgcurrent::after {
  background-size: 8px;
}

.weglot-switcher--m .wg-drop.country-selector .wgcurrent a {
  padding-right: 15px;
}
.weglot-switcher--m .wg-drop.country-selector ul {
  min-width: unset;
  right: unset;
  left: 0;
}
.weglot-switcher--m .wg-drop.country-selector a {
  padding-right: 0;
}
.weglot-switcher--m .wg-flags a img.wg-flag {
  height: 20px;
}

/* --- RC Subscription Widget --- */

#rc_login {
  display: none !important;
}

.rc-container {
  margin: 1rem 0;
}
div.rc-widget .rc_widget__option {
  min-height: 3rem;
  display: flex;
  flex-direction: column;
  align-items: flex-start;
  justify-content: center;
  border-radius: .5rem;
  -webkit-border-radius: .5rem;
}
.rc-widget .rc-radio__label .rc-option__price.rc_widget__price {
  font-weight: 600;
}
.rc-widget .rc-template__legacy-radio .rc_widget__option__selector + .rc-selling-plans {
  padding-left: 20px;
}
.rc-widget .rc-radio__label .rc-option__price.rc_widget__price::before {
  content: "\2014";
  display: inline-block;
  margin: 0 .75rem 0 .5rem;
  font-weight: normal;
}
div.rc-widget .rc-selling-plans.rc_widget__option__plans .rc_widget__option__plans__dropdown {
  padding: .25rem .5rem;
  background-color: #d7d7d7;
  border-radius: 10rem;
  -webkit-border-radius: 10rem;
}

